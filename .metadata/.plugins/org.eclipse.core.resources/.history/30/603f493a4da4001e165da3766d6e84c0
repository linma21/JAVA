package sub3;
 /*
  * 
  *  날짜 : 23/12/26
  *  이름 : 최이진
  *  내용 : JAVA 연산자 실습
  * 
  *  연산자(Operate)
  *   - 변수의 데이터를 처리하기 위한 연산식
  *    -일반적으로 산술, 복합대인 , 비교 , 논리연산을 수행
  * 
  * 
  */
public class OperateTest {
	public static void main(String[] args) {
		
		
		//산술연산자
		int num1 = 1;
		int num2 = 2;
		int num3 = 3;
		int num4 = 4;
		
		int result1 = num1 + num2;
		int result2 = num1 - num2;
		int result3 = num2 * num3;
		int result4 = num4 / num2;
		int result5 = num4 % num3;
		int result6 = 9 % num3;
		
		System.out.println("result1 : " + result1);
		System.out.println("result2 : " + result2);
		System.out.println("result3 : " + result3);
		System.out.println("result4 : " + result4);
		System.out.println("result5 : " + result5);
		System.out.println("result6 : " + result6);
			
		//증감연산자
		int num = 0;
		
		num++;
		++num;
		
		System.out.println("num : " + num);
		
		num--;
		--num;
		System.out.println("num : " + num);
		
		//복합대인연산자 기존 값에 대입해서 계산
		int n1 = 1, n2 = 2, n3 = 3, n4 = 4;
		
		n1 += 1;
		n2 -= 2;
		n3 *= 3;
		n4 /= 4;
		n4 += 2;
		n4 *= 3;
	
		System.out.println("n1 : " + n1);
		System.out.println("n2 : " + n2);
		System.out.println("n3 : " + n3);
		System.out.println("n4 : " + n4);
			
		
		//비교연산자
		int var1 = 1;
		
		int var2 = 2;
		
		boolean rs1 = var1 > var2;
		boolean rs2 = var1 < var2;
		boolean rs3 = var1 >= var2;
		boolean rs4 = var1 <= var2;
		boolean rs5 = var1 == var2;
		boolean rs6 = var1 != var2;
		// 틀림 > false ??? = boolean
		
		
		
		
		//논리연산자
		
		
		
	}

}
