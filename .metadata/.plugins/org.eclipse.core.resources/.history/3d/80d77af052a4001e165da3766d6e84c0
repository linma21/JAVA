package sub1;

public class VariableTest2 {
	
	public static void main(String[] args) {
		
		int a = 3;
		int b = 7;
		String c = "호랑이";

		
		System.out.println("a =" + a);
		System.out.println("b =" + b);
		System.out.println("c =" + c);
		
		
	    a = 5;

	    System.out.println("a =" + a);
	    
	  
       final int NUM = 5;
       
       System.out.println("NUM =" + NUM);
       
		
		
		
		
	}
	
	
	

}
