package sub3;
/*
 * 날짜 : 23/12/27
 * 이름 : 최이진
 * 내용 : 반복문 for 실행
 */
public class Fortest {

	public static void main(String[] args) {
		
		//for(초기식 : 조건식 : 증감식)
		for(int i=1 ; i<=5 ; i++) {
			
		System.out.println("i : " + i);
		
			
			
		}
		//1부터 10까지 합
		//1부터 10까지 짝수합
		//for 중첩
		//구구단
		//별삼각형
		
		
		
		
		
	}
	
	
}
